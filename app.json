{
  "expo": {
    "name": "BiblioTube",
    "slug": "bibliotube",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "bibliotube",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.angelmayor.bibliotube",
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "https",
              "host": "*.youtube.com",
              "pathPattern": "/watch.*"
            },
            {
              "scheme": "https",
              "host": "youtu.be",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "www.youtube.com",
              "pathPattern": "/watch.*"
            },
            {
              "scheme": "https",
              "host": "m.youtube.com",
              "pathPattern": "/watch.*"
            },
            {
              "scheme": "https",
              "host": "*.instagram.com",
              "pathPattern": "/p/.*"
            },
            {
              "scheme": "https",
              "host": "*.instagram.com",
              "pathPattern": "/reel/.*"
            },
            {
              "scheme": "https",
              "host": "*.tiktok.com",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "vm.tiktok.com",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "vt.tiktok.com",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "m.tiktok.com",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "www.tiktok.com",
              "pathPattern": "/.*"
            },
            {
              "scheme": "https",
              "host": "*.facebook.com",
              "pathPattern": "/.*"
            }
          ]
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          }
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.angelmayor.bibliotube",
      "icon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8361a37a-c645-45dd-8bec-21dad19ea9bd"
      }
    },
    "owner": "angelaramiz"
  }
}
